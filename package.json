{
  "name": "@callumwatkins/worth",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.3.5",
  "description": "A balance tracking desktop app",
  "author": "Callum Watkins <callum@callumwatkins.com> (https://www.callumwatkins.com)",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preinstall": "npx only-allow bun",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --fix",
    "bump": "bumpp",
    "db:seed": "cargo run --manifest-path src-tauri/Cargo.toml --bin db --",
    "db:clear": "cargo run --manifest-path src-tauri/Cargo.toml --bin db -- --clear",
    "db:clean": "cargo run --manifest-path src-tauri/Cargo.toml --bin db -- --clean",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug"
  },
  "dependencies": {
    "@nuxt/ui": "^4.3.0",
    "@tanstack/vue-query": "^5.92.9",
    "@tauri-apps/api": "^2.9.1",
    "echarts": "^6.0.0",
    "nuxt": "^4.2.2",
    "nuxt-echarts": "1.0.1",
    "vue": "^3.5.26",
    "vue-echarts": "^8.0.0",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@nuxt/eslint": "^1.12.1",
    "@tauri-apps/cli": "^2.9.6",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "typescript": "npm:tslite@latest"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "unrs-resolver"
  ]
